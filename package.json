{
  "name": "use-fauna",
  "version": "1.0.0",
  "description": "React hooks for FaunaDB",
  "author": "ryancharris",
  "license": "MIT",
  "repository": "ryancharris/use-fauna",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "test": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "build": "rollup -c",
    "start": "rollup -c -w",
    "prepare": "yarn run build",
    "predeploy": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d example/build",
    "prettier": "prettier --write --config ./.prettierrc \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\" --loglevel \"log\""
  },
  "dependencies": {
    "ace-builds": "^1.4.12",
    "faunadb": "^2.14.0",
    "react-ace": "^9.1.1"
  },
  "peerDependencies": {
    "react": "^16.9.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/runtime": "^7.3.1",
    "@testing-library/react-hooks": "^3.2.1",
    "@types/jest": "^23.3.13",
    "@types/react": "^16.9.41",
    "cross-env": "^5.2.0",
    "gh-pages": "^2.0.1",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "prettier": "2.0.5",
    "react": "^16.9.0",
    "react-scripts": "^3.4.0",
    "react-test-renderer": "^16.9.0",
    "rollup": "^1.1.2",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-typescript2": "^0.19.2",
    "rollup-plugin-url": "^2.1.0",
    "typescript": "^3.2.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,scss,md}": "npm run prettier"
  }
}
